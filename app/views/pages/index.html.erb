<div class="container">
    <h2>Ballot America Partner Portal</h2>
    <%= link_to "Become a partner", new_partner_registration_path, class: "btn btn-info" %>
    <% if partner_signed_in? && current_partner %>
        <%= link_to "Dashboard", partners_clients_path, class: "btn btn-warning" %>
        <%= link_to "Logout", destroy_partner_session_path, method: :delete, class: "btn btn-danger" %>
    <% else %>
        <%= link_to "Login", new_partner_session_path, class: "btn btn-primary" %>
    <% end %>
</div>